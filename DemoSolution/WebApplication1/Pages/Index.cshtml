@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<table>
    <thead>
    <tr>
        <th>Type</th>
        <th>Value</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var claim in User.Claims)
    {
    <tr>
        <td>@claim.Type</td>
        <td>@claim.Value</td>
    </tr>
    }
    </tbody>
</table>